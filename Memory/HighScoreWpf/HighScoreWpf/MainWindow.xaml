<Window x:Class="HighScoreWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HighScoreWpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
      
        </Grid.RowDefinitions>

        <Label Content="Enter some text" Margin="5"/>
        <TextBox Grid.Row="1" Name="MyTextBox" Margin="5"/>
        <TextBox Grid.Row="2" Name="MyTextBox2" Margin="0,8,9.6,2.4"/>
        <Button Grid.Row="5" Content="OK" Margin="0,9.6,9.6,0" Click="Button_Click"/>
   
        <ListView Margin="10" Name="lvDataBinding">
        </ListView>
    </Grid>
</Window>
